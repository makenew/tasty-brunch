  },
  "scripts": {
    "brunch": "brunch",
    "flow": "flow",
    "gulp": "gulp",
    "mocha": "mocha",

    "test": "mocha --opts mocha.opts app/**/*.spec.js",
      "pretest": "gulp lint && flow",

    "build": "brunch build --production",
      "prebuild": "gulp clean",
      "postbuild": "gulp htmlhint",

    "dist": "npm run build",
      "predist": "npm test",
      "postdist": "gulp rev",

    "deploy": "gulp deploy",
      "predeploy": "npm run dist",

    "start": "brunch watch --server",
    "watch": "gulp watch"
  },
  "files": [
    "public"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
